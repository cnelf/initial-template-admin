{
  "name": "huaer-template-admin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "dev": "vue-cli-service serve",
    "build:stage": "vue-cli-service build --mode staging",
    "build:prod": "vue-cli-service build --mode production",
    "prettier": "prettier --write .",
    "lint": "vue-cli-service lint src --no-fix --max-warnings 0",
    "lint:style": "stylelint \"**/*.{vue,less,postcss,css,scss}\" --no-fix --max-warnings 0",
    "lint:style:fix": "stylelint \"**/*.{vue,less,postcss,css,scss}\" --fix",
    "report": "vue-cli-service build --report"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.md": [
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{css,scss,less,styl}": [
      "prettier --write",
      "stylelint --fix"
    ],
    "*.vue": [
      "prettier --write",
      "eslint --fix",
      "stylelint --fix"
    ]
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "core-js": "^3.6.5",
    "dayjs": "^1.10.8",
    "echarts": "^5.3.1",
    "element-ui": "^2.15.6",
    "lodash-es": "^4.17.21",
    "nprogress": "^0.2.0",
    "qs": "^6.10.3",
    "v-clipboard": "^2.2.3",
    "vue": "^2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.15",
    "@vue/cli-plugin-eslint": "~4.5.15",
    "@vue/cli-plugin-router": "~4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.15",
    "@vue/cli-service": "~4.5.15",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "compression-webpack-plugin": "^6.1.1",
    "eslint": "^7.32.0",
    "eslint-config-huaer": "^1.0.3",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.3.8",
    "image-webpack-loader": "^8.1.0",
    "lint-staged": "^12.3.5",
    "prettier": "^2.5.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^5.0.0",
    "svg-sprite-loader": "^6.0.11",
    "vue-cli-plugin-windicss": "~1.1.3",
    "vue-template-compiler": "^2.6.11"
  }
}
